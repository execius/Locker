CC=clang

Locker :
	$(CC) -lm -lssl -lcrypto -lcjson -lb64  headers/eror_handling/eror_handling.c	utilities/initialize/initialize.c    utilities/configs/config_parser/config_parser.c utilities/str_utils/strutils.c utilities/json_utils/json_utils.c security/encryption/encryptdecrypt.c security/encryption/hashing.c security/user_handling/login/login.c utilities/hex_bin_utils/hex_bin_utils.c security/user_handling/user_utils/user_utils.c security/user_handling/make_user/make_user.c  utilities/configs/config_writer/config_writer.c headers/defined_values/defined_values.c headers/defined_values/arrays_counters.c Locker.c -I utilities/initialize   -I headers/eror_handling   -I ./utilities/configs/config_parser/ -I headers/defined_values/ -I headers/includes/  -I utilities/json_utils/ -I security/encryption/ -I security/user_handling/login/ -I utilities/hex_bin_utils/ -I security/user_handling/user_utils/ -I security/user_handling/make_user/ -I utilities/configs/config_writer/ -I utilities/str_utils/ -o Locker

optimization_testing :
	$(CC) -O3  -lm -lssl -lcrypto -lcjson -lb64 headers/eror_handling/eror_handling.c	utilities/initialize/initialize.c    utilities/configs/config_parser/config_parser.c utilities/str_utils/strutils.c utilities/json_utils/json_utils.c security/encryption/encryptdecrypt.c security/encryption/hashing.c security/user_handling/login/login.c utilities/hex_bin_utils/hex_bin_utils.c security/user_handling/user_utils/user_utils.c security/user_handling/make_user/make_user.c  utilities/configs/config_writer/config_writer.c headers/defined_values/defined_values.c headers/defined_values/arrays_counters.c Locker.c -I utilities/initialize   -I headers/eror_handling   -I ./utilities/configs/config_parser/ -I headers/defined_values/ -I headers/includes/  -I utilities/json_utils/ -I security/encryption/ -I security/user_handling/login/ -I utilities/hex_bin_utils/ -I security/user_handling/user_utils/ -I security/user_handling/make_user/ -I utilities/configs/config_writer/ -I utilities/str_utils/ -o optimization_testing
debug :
	$(CC)  -g -Wall  -pedantic -lm -lssl -lcrypto -lcjson -lb64 headers/eror_handling/eror_handling.c	utilities/initialize/initialize.c    utilities/configs/config_parser/config_parser.c utilities/str_utils/strutils.c utilities/json_utils/json_utils.c security/encryption/encryptdecrypt.c security/encryption/hashing.c security/user_handling/login/login.c utilities/hex_bin_utils/hex_bin_utils.c security/user_handling/user_utils/user_utils.c security/user_handling/make_user/make_user.c  utilities/configs/config_writer/config_writer.c headers/defined_values/defined_values.c headers/defined_values/arrays_counters.c Locker.c -I utilities/initialize   -I headers/eror_handling   -I ./utilities/configs/config_parser/ -I headers/defined_values/ -I headers/includes/  -I utilities/json_utils/ -I security/encryption/ -I security/user_handling/login/ -I utilities/hex_bin_utils/ -I security/user_handling/user_utils/ -I security/user_handling/make_user/ -I utilities/configs/config_writer/ -I utilities/str_utils/ -o debug

